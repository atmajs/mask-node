UTest({
	'attributes' () {
		var tmpl = `script type='text/mask'>'>div<'`
		var html = mask.render(tmpl);
		
		has_(html, 'text/mask');
		has_(html, '>div<');
	},
	'do not interpolate' () {
		var tmpl = `script type='text/mask'>' div > "~[foo]" '`
		var html = mask.render(tmpl);
		
		has_(html, ' div > "~[foo]" ');
	}
})