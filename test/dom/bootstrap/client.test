var pathClientCompo = '/test/tmpl/compos/clientCompo.es6';

UTest({
	$config: {
		'http.include': [
			pathClientCompo
		]
	},	
	'clientCompo' () {
		return $render(':clientCompo;', {
			scripts: [
				pathClientCompo
			]
		}).pipe(el => {
			$(el)
				.has_('#foo')
				.eq_ ('text', 'FooTitle');
		});
	}
})