{
  "name": "-",
  "description": "MaskJS for Node.JS",
  "version": "0.60.0",
  "homepage": "http://atmajs.com/mask",
  "contributors": [],
  "author": "Alexander Kit <alex.kit@atmajs.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/atmajs/mask-node.git"
  },
  "bugs": {
    "url": "https://github.com/atmajs/mask-node/issues"
  },
  "licenses": [
    "MIT"
  ],
  "main": "./lib/mask.node.js",
  "devDependencies": {
    "atma": "^0.10.48",
    "atma-loader-babel": "^1.1.28",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-preset-es2015": "^6.16.0",
    "includejs": "latest"
  },
  "scripts": {
    "test": "atma test",
    "build": "atma",
    "server": "atma server",
    "examples": "atma custom examples/index"
  },
  "keywords": [],
  "engines": {
    "node": "~0.8.9"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "test",
          "es6"
        ],
        "babel": {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "async-to-promises"
          ]
        }
      }
    }
  }
}
